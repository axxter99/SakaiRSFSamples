<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://maven.apache.org/maven-v3_0_0.xsd">
  <extend>${basedir}/../project.xml</extend>
  <pomVersion>3</pomVersion>
  <name>Sakai RSF Hibernate Cookbook Component</name>
  <artifactId>sakai-rsfcookbook-component</artifactId>
  <shortDescription>Sakai RSF Hibernate Cookbook Component</shortDescription>
  <description>
	This component consists simply of an AdditionHibernateMappingsImpl 
  Spring declaration, in order to export the Hibernate mappings for the tool
  into the Global Sakai Hibernate SessionFactory.
  </description>
  <properties>
	<deploy.type>components</deploy.type>
  </properties>
  <build>
    <nagEmailAddress>antranig@caret.cam.ac.uk</nagEmailAddress>
   
  </build>
   <dependencies>
    <dependency>
      <groupId>rsftest</groupId>
      <artifactId>rsf-hibernate-cookbook-category-datamodel</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <properties>
        <deploy.target>shared</deploy.target>
      </properties>
    </dependency>
  </dependencies>
</project>
